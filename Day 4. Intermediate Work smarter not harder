#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t>0){
	    int l,a,b;
	    cin>>l>>a>>b;
	  int c,d;

	    if(l%a==0){
	        c=l/a;
	    }
	    else{
	       c=(l/a)+1;
	    }
	    if(l%b==0){
	        d=l/b;
	    }
	    else{
	        d=(l/b)+1;
	    }
	    if(c>d){
	        cout<<c-d-1<<endl;
	    }
	    else{
	        cout<<"-1"<<endl;
	    }
	    t--;
	}
	return 0;
}
