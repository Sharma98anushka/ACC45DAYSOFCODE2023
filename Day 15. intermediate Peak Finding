#include <iostream>
#include<algorithm>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t>0){
	    int n,max=0;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	       if(a[i]>max){
	           max=a[i];
	       }
	           
	    }
	    
	    cout<<max<<endl;
	    t--;
	}
	return 0;
}
