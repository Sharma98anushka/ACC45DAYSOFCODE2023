#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a1,a2,b1,b2;
	    cin>>a1>>a2>>b1>>b2;
	    int a=a1-a2;
	    int b=b1-b2;
	    int c=a2+b2-a1-b1;
	    if(c>a+b){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	    
	}
	return 0;
}
